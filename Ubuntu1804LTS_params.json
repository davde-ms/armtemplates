{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "virtualMachineNamePrefix": {
            "value": "yournodenameprefix"
        },
        "virtualMachineCount": {
            "value": 1
        },
        "virtualMachineSize": {
            "value": "Standard_NC6"
        },
        "storageAccountType": {
            "value": "Standard_LRS"
        },
        "diskSizeinGB": {
            "value": "128"
        },
        "adminUsername": {
            "value": "theAdminUserName"
        },
        "adminPublicKey": {
            "value": "ssh-rsa AAAAB3NzaC1yc2EAAAADAQ (...)"
        },
        "virtualNetworkName": {
            "value": "yourAzureVirtualNetworkName"
        },
        "vnetAddressSpace": {
            "value": "10.0.0.0/16"
        },
        "subnetName": {
            "value": "dsvm-subnet"
        },
        "subnetPrefix": {
            "value": "10.0.0.0/24"
        },
        "dnsPrefixForPublicIP": {
            "value": "ipprefix"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "autoShutdownStatus": {
            "value": "Enabled"
        },
        "autoShutdownTime": {
            "value": "21:00"
        },
        "autoShutdownTimeZone": {
            "value": "Eastern Standard Time"
        },
        "autoShutdownNotificationStatus": {
            "value": "Disabled"
        }
    }
}