{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "virtualMachineNamePrefix": {
            "value": "yourNodePrefixName"
        },
        "virtualMachineCount": {
            "value": 3
        },
        "virtualMachineSize": {
            "value": "Standard_NC6"
        },
        "storageAccountType" : {
            "value": ""
        },
        "diskSizeinGB" : {
            "value": "128"
        },
        "adminUsername": {
            "value": "theAdminUserName"
        },
        "adminPublicKey" : {
            "value": "Copy here your public ssh key value, something that begins with ssh-rsa and should look like: ssh-rsa "
        },
        "virtualNetworkName": {
            "value": "yourAzureVirtualNetworkName"
        },
        "vnetAddressSpace": {
            "value": "10.0.0.0/16"
        },
        "subnetName": {
            "value": "dsvm-subnet"
        },
        "subnetPrefix": {
            "value": "10.0.0.0/24"
        },
        "dnsPrefixForPublicIP": {
            "value": "yourPublicIPAddressNamePrefix"
        },
        "publicIpAddressSku": {
            "value": "Basic"
        },
        "autoShutdownStatus": {
            "value": "Enabled"
        },
        "autoShutdownTime": {
            "value": "21:00"
        },
        "autoShutdownTimeZone": {
            "value": "Eastern Standard Time"
        },
        "autoShutdownNotificationStatus": {
            "value": "Disabled"
        }
    }
}